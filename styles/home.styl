#feature-1
  position: relative
  background-color: #ECEFF4
  min-height: 90vh
  margin: 0 auto
  padding: 0.5rem 0.75rem 0.5rem 0.75rem
  @media screen and (min-width 1024px)
    padding: 0.5rem 1rem 0.5rem 1rem
    min-height: 20vh
  @media screen and (min-width 1280px)
    padding: 0
    min-height: 70vh

#contact
  position: relative
  background-color: white
  min-height: 90vh
  margin: 0 auto
  padding: 0.5rem 0.75rem 0.5rem 0.75rem
  @media screen and (min-width 1024px)
    padding: 0.5rem 1rem 0.5rem 1rem
    min-height: 20vh
  @media screen and (min-width 1280px)
    padding: 0 0 2.75rem 0
    min-height: 70vh

.custom-shape-divider-top-1604376211
    position: absolute
    top: 0
    left: 0
    width: 100%
    overflow: hidden
    line-height: 0

.custom-shape-divider-top-1604376211 svg
    position: relative
    display: block
    width: calc(100% + 1.3px)
    height: 41px
    transform: rotateY(180deg)

.custom-shape-divider-top-1604376211 .shape-fill
    fill: white

#feature-one-container
  margin: 0 auto
  padding-top: 3rem
  display: block
  max-width: 1250px
  @media screen and (min-width 1024px)
    display: flex
    align-items: center
    justify-content: space-between

#feature-one-container #summary
  width: 100%
  text-align: center
  @media screen and (min-width 1024px)
    width: 50%
    text-align: left

#feature-one-container #summary #description
  width: 100%
  text-align: center
  @media screen and (min-width 1024px)
    width: 80%
    text-align: left

#feature-one-container #images__sm
  width: 100%
  display: block
  @media screen and (min-width 1024px)
    display: none

#feature-one-container #images__lg
  width: 50%
  display: none
  @media screen and (min-width 1024px)
    display: block

#feature-one-container #images__sm #mockup-1
  text-align: right

#feature-one-container #images__sm #mockup-1 #title-right
  text-align: right
  font-size: 14px
  font-weight: 600

#feature-one-container #images__sm #mockup-2
  text-align: left

#feature-one-container #images__sm #mockup-2 #title-left
  text-align: left
  font-size: 14px
  font-weight: 600

#feature-one-container #images__lg #mockup-1
  text-align: right

#feature-one-container #images__lg #mockup-1 #title-right
  text-align: right
  font-size: 14px
  font-weight: 600

#feature-one-container #images__lg #mockup-2
  text-align: left

#feature-one-container #images__lg #mockup-2 #title-left
  text-align: left
  font-size: 14px
  font-weight: 600

